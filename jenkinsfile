pipeline{
    agent any
    stages{       
       stage('Build Stage'){
            steps{
                sh 'mvn clean install'
            }
         }
        stage('SonarQube Analysis Stage') {
            steps{
                withSonarQubeEnv('sonarQube') { 
                    sh "mvn clean verify sonar:sonar -Dsonar.projectKey=sonar-sonar"
                }
            }
        }
    }
}
